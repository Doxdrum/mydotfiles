@article{budinich18_compl_repres_cliff_algeb,
  author =	 {Budinich, Marco},
  title =	 {On Complex Representations of Clifford Algebra},
  journal =	 {CoRR},
  year =	 2018,
  url =		 {http://arxiv.org/abs/1805.03126v1},
  abstract =	 {We show that complex representations of Clifford
                  algebra can always be reduced either to a real or to
                  a quaternionic algebra depending on signature of
                  complex space thus showing that complex spinors are
                  unavoidably either real Majorana spinors or
                  quaternionic spinors. We use this result to support
                  (1,3) signature for Minkowski space.},
  archivePrefix ={arXiv},
  eprint =	 {1805.03126},
  primaryClass = {math-ph},
