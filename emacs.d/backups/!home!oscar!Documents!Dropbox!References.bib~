@article{morales16_dimen_compac_chern_simon_theor,
  author =	 {Morales, Ivan and Neves, Bruno and Oporto, Zui and
                  Piguet, Olivier},
  title =	 {Dimensionally Compactified Chern-Simon Theory in 5d
                  As a Gravitation Theory in 4d},
  year =	 {2016},
  url =		 {http://arxiv.org/abs/1612.00409v1},
  abstract =	 {We propose a gravitation theory in 4 dimensional
                  space-time obtained by compacting to 4 dimensions
                  the five dimensional topological Chern-Simons theory
                  with the gauge group SO(1,5) or SO(2,4) -- the de
                  Sitter or anti-de Sitter group of 5-dimensional
                  space-time. In the resulting theory, torsion, which
                  is solution of the field equations as in any
                  gravitation theory in the first order formalism, is
                  not necessarily zero. However, a cosmological
                  solution with zero torsion exists, which reproduces
                  the Lambda-CDM cosmological solution of General
                  Relativity. A realistic solution with spherical
                  symmetry is also obtained.},
  archivePrefix ={arXiv},
  eprint =	 {1612.00409},
  primaryClass = {gr-qc},
}



