@article{giddings18_quant_first_gravit,
  author =	 {Giddings, Steven B.},
  title =	 {Quantum-First Gravity},
  journal =	 {CoRR},
  year =	 2018,
  url =		 {http://arxiv.org/abs/1803.04973v1},
  abstract =	 {This paper elaborates on an intrinsically quantum
                  approach to gravity, which begins with a general
                  framework for quantum mechanics and then seeks to
                  identify additional mathematical structure on
                  Hilbert space that is responsible for gravity and
                  other phenomena. A key principle in this approach is
                  that of correspondence: this structure should
                  reproduce spacetime, general relativity, and quantum
                  field theory in a limit of weak gravitational
                  fields. A central question is that of "Einstein
                  separability," and asks how to define mutually
                  independent subsystems, e.g. through
                  localization. Familiar definitions involving tensor
                  products or operator subalgebras do not clearly
                  accomplish this in gravity, as is seen in the
                  correspondence limit. Instead, gravitational
                  behavior, particularly gauge-invariance, suggests a
                  network of Hilbert subspaces related via inclusion
                  maps. Any such localization structure is also
                  expected to place strong constraints on evolution,
                  which are also supplemented by the constraint of
                  unitarity.},
  archivePrefix ={arXiv},
  eprint =	 {1803.04973},
  primaryClass = {hep-th},
}



